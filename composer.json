{
    "name": "grimzy/jwt-service-provider",
    "description": "Pimple/Silex Service Provider for generating JSON Web Tokens (JWT).",
    "type": "library",
    "authors": [
        {
            "name": "Joseph Estefane",
            "email": "estefanejoe@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "pimple/pimple": "^3.0",
        "firebase/php-jwt": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.5"
    },
    "autoload": {
        "psr-4": {
            "Grimzy\\JWTServiceProvider\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": { "Grimzy\\JWTServiceProvider\\Tests\\" : "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
